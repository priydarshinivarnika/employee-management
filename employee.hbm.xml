<?xml version='1.0' encoding='UTF-8'?>  
<!DOCTYPE hibernate-mapping PUBLIC  
 "-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.common.details.Employee" table="one">
		<id name="empId">
			<generator class="assigned"></generator>
		</id>

		<property name="empFirstName"></property>
		<property name="empLastName"></property>
		<property name="empSkill"></property>
		<property name="empDepartment"></property>


	</class>

</hibernate-mapping>  
